{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/components/ui/*": [
        "src/components/ui/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/lib/utils": [
        "src/lib/utils"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "types": [
      "vite/client"
    ]
  }
}
